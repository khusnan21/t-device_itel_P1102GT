# Fstab
ro.postinstall.fstab.prefix=/system

# USB MTP ADB
ro.sys.usb.storage.type=mtp,adb

# Crypto
ro.crypto.volume.filenames_mode=aes-256-cts

# Gatekeeper
ro.hardware.gatekeeper=trustkernel 
ro.hardware.kmsetkey=trustkernel 

# TEE
ro.vendor.mtk_tee_gp_support=1
ro.vendor.mtk_trustkernel_tee_support=1

# Force use keymint instead of keymaster
ro.hardware.keymint=trustkernel